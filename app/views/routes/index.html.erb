<div id="container" style="width:960px; background-color:#EEEEEE; margin-left:auto; margin-right:auto; " >
  <h1>Listing routes</h1>

  <table>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>

  <% @routes.each do |route| %>
    <tr>
      <td><%= route.name %></td>
      <td><%= link_to 'Show', route %></td>
      <td><%= link_to 'Edit', edit_route_path(route) %></td>
      <td><%= link_to 'Destroy', route, confirm: 'Are you sure?', method: :delete %></td>
    </tr>
  <% end %>
  </table>

  <br />

  <%= link_to 'New Route', new_route_path %>
</div>